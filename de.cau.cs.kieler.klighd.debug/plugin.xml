<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.cau.cs.kieler.klighd.debugVisualization" name="Debug Visualization" schema="schema/debugVisualization.exsd"/>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.klighd.debug.KlighdDebugPlugin">
      </startup>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.modelTransformations">
      <transformation
            class="de.cau.cs.kieler.klighd.transformations.GuiceBasedTransformationFactory:de.cau.cs.kieler.klighd.debug.transformations.KlighdDebugTransformation"
            id="de.cau.cs.kieler.klighd.debug.transformation">
      </transformation>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.debugVisualization">
      <visualization
            class="java.util.ArrayDeque"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ArrayDequeTransformation">
      </visualization>
      <visualization
            class="java.lang.Boolean"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="java.lang.Character"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="java.util.ArrayList"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ArrayListTransformation">
      </visualization>
      <visualization
            class="java.util.BitSet"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.BitSetTransformation">
      </visualization>
      <visualization
            class="java.util.EnumMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.EnumMapTransformation">
      </visualization>
      <visualization
            class="java.util.EnumSet"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.EnumSetTransformation">
      </visualization>
      <visualization
            class="java.util.HashMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.KeyValueTransformation">
      </visualization>
      <visualization
            class="java.util.HashSet"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.HashSetTransformation">
      </visualization>
      <visualization
            class="java.util.IdentityHashMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.IdentityHashMapTransformation">
      </visualization>
      <visualization
            class="java.util.Hashtable"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.KeyValueTransformation">
      </visualization>
      <visualization
            class="java.util.LinkedHashMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.LinkedHashMapTransformation">
      </visualization>
      <visualization
            class="java.util.LinkedList"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.LinkedListTransformation">
      </visualization>
      <visualization
            class="java.util.PriorityQueue"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.PriorityQueueTransformation">
      </visualization>
      <visualization
            class="java.util.TreeMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.TreeMapTransformation">
      </visualization>
      <visualization
            class="java.util.TreeSet"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.TreeSetTransformation">
      </visualization>
      <visualization
            class="java.util.Stack"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.VectorTransformation">
      </visualization>
      <visualization
            class="java.util.Vector"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.VectorTransformation">
      </visualization>
      <visualization
            class="java.util.WeakHashMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.WeakHashMapTransformation">
      </visualization>
      <visualization
            class="java.lang.Number"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
   </extension>

</plugin>
