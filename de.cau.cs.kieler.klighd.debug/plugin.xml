<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.klighd.debug.KlighdDebugPlugin">
      </startup>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.modelTransformations">
      <transformation
            class="de.cau.cs.kieler.klighd.transformations.GuiceBasedTransformationFactory:de.cau.cs.kieler.klighd.debug.ecore.EcoreDiagramSynthesis"
            id="de.cau.cs.kieler.klighd.debug.transformation1">
      </transformation>
      <transformation
            class="de.cau.cs.kieler.klighd.transformations.GuiceBasedTransformationFactory:de.cau.cs.kieler.klighd.debug.transformations.LinkedListDiagramSynthesis"
            id="de.cau.cs.kieler.klighd.debug.transformation2">
      </transformation>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <command
               commandId="de.cau.cs.kieler.klighd.debug.command1"
               label="test"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.cau.cs.kieler.klighd.debug.testHandler"
            id="de.cau.cs.kieler.klighd.debug.command1"
            name="name">
      </command>
   </extension>

</plugin>
