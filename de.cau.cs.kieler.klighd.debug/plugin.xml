<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.cau.cs.kieler.klighd.debugVisualization" name="KlighD Debug Visualization" schema="schema/KlighdDebugVisualization.exsd"/>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.klighd.debug.KlighdDebugPlugin">
      </startup>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.modelTransformations">
      <transformation
            class="de.cau.cs.kieler.klighd.transformations.GuiceBasedTransformationFactory:de.cau.cs.kieler.klighd.debug.transformations.KlighdDebugTransformation"
            id="de.cau.cs.kieler.klighd.debug.transformation">
      </transformation>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.debugVisualization">
      <visualization
            class="java.util.LinkedList"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.LinkedListTransformation">
      </visualization>
      <visualization
            class="java.util.HashMap"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.HashMapTransformation">
      </visualization>
      <visualization
            class="java.lang.Boolean"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="java.lang.Integer"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="java.lang.Byte"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="java.lang.Character"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.ObjectValueTransformation">
      </visualization>
      <visualization
            class="de.cau.cs.kieler.klay.layered.graph.LGraph"
            package="de.cau.cs.kieler.klay.layered.graph"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.LGraphTransformation">
      </visualization>
      <visualization
            class="de.cau.cs.kieler.klay.layered.graph.LNode"
            package="de.cau.cs.kieler.klay.layered.graph"
            transformation="de.cau.cs.kieler.klighd.debug.transformations.LNodeTransformation">
      </visualization>
   </extension>

</plugin>
